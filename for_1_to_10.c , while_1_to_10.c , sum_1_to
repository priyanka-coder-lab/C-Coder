
#include <stdio.h>

int main() {
    int i;
    for(i = 1; i <= 10; i++) {
        printf("%d\n", i);
    }
    return 0;
}

while_1_to_10.c

  #include <stdio.h>

int main() {
    int i = 1;
    while(i <= 10) {
        printf("%d\n", i);
        i++;
    }
    return 0;
}

sum_1_to_10.c

  #include <stdio.h>

int main() {
    int i, sum = 0;

    for(i = 1; i <= 10; i++) {
        sum = sum + i;
    }

    printf("Sum = %d", sum);
    return 0;
}

  
